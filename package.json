{
  "name": "flipper",
  "version": "2.0.28",
  "description": "A futurstic inventory system",
  "homepage": "https://github.com/maximegris/angular-electron",
  "author": {
    "name": "flipper",
    "email": "yegobox@gmail.com"
  },
  "keywords": [
    "pos",
    "linux"
  ],
  "main": "main.js",
  "scripts": {
    "config": "ts-node ./src/assets/setEnv.ts",
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build -c production --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build -p  always --linux snap -c.snap.publish=github",
    "electron:windows-32": "npm run build:prod && npx electron-builder build --ia32  --windows",
    "electron:windows-64": "npm run build:prod && npx electron-builder build --x64  --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build -p always  --mac",
    "electron:build": "npm run build:prod && electron-builder build",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "build-vendors": "ng build flippervendors --watch",
    "build-dialog": "ng build flipperdialog --watch",
    "build-event": "ng build flipperevent --watch",
    "build-color": "ng build flippercolor --watch",
    "build-component": "ng build flippercomponents --watch",
    "build-dashboard": "ng build flipperdashboard --watch",
    "build-font": "ng build flipperfont --watch",
    "build-stock": "ng build flipperinventory --watch",
    "build-pos": "ng build flipperpos --watch ",
    "build-db": "ng build flipperofflinedatabase --watch",
    "build-setting": "ng build flippersettings --watch",
    "build-upload": "ng build flipperfileupload --watch",
    "build-menu": "ng build flippermenu --watch",
    "build-card": "ng build paymentcard --watch",
    "release": "standard-version",
    "alpha": "npm run release -- --prerelease alpha",
    "major": "npm run release -- --release-as major",
    "minor": "npm run release -- --release-as minor",
    "patch": "npm run release -- --release-as patch",
    "buildAll": " ng build flippercolor && ng build flipperevent  && ng build flipperfont && ng build flipperfileupload   && ng build paymentcard  && ng build flippercomponents && ng build flippervendors     && ng build flipperdialog   && ng build flipperpos",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^11.0.0",
    "@angular/animations": "~11.0.3",
    "@angular/cdk": "^11.1.1",
    "@angular/common": "~11.0.3",
    "@angular/compiler": "~11.0.3",
    "@angular/core": "~11.0.3",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.0.3",
    "@angular/localize": "^11.1.2",
    "@angular/material": "^11.1.1",
    "@angular/platform-browser": "~11.0.3",
    "@angular/platform-browser-dynamic": "~11.0.3",
    "@angular/router": "~11.0.3",
    "@enexus/flipper-color": "0.0.3",
    "@enexus/flipper-components": "^1.6.58",
    "@enexus/flipper-dashboard": "1.0.2",
    "@enexus/flipper-dialog": "1.0.3",
    "@enexus/flipper-event": "1.0.5",
    "@enexus/flipper-file-upload": "1.0.2",
    "@enexus/flipper-font": "1.0.2",
    "@enexus/flipper-inventory": "^1.4.3",
    "@enexus/flipper-menu": "^1.2.6",
    "@enexus/flipper-offline-database": "1.0.13",
    "@enexus/flipper-pos": "^1.3.0",
    "@enexus/flipper-settings": "1.4.12",
    "@enexus/flipper-vendors": "1.1.1",
    "@enexus/flipperofflinedatabase": "1.2.13",
    "@enexus/payment-card": "1.0.3",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@types/pouchdb": "^6.4.0",
    "alasql": "^0.6.5",
    "an-qrcode": "1.0.5",
    "auto-launch": "5.0.5",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.3.1",
    "electron-push-receiver": "^2.1.3",
    "electron-updater": "^4.3.5",
    "ngx-barcode": "^0.2.4",
    "node-notifier": "^9.0.0",
    "npm-run-all": "4.1.5",
    "pouchdb": "^7.2.1",
    "pouchdb-debug": "^7.2.1",
    "pouchdb-find": "^7.2.2",
    "pusher-js": "^6.0.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.3",
    "@angular/cli": "~11.0.3",
    "@angular/compiler-cli": "~11.0.3",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.19.16",
    "codelyzer": "^6.0.0",
    "conventional-changelog-cli": "2.1.1",
    "electron": "11.0.3",
    "electron-builder": "22.9.1",
    "electron-reload": "1.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.1.3",
    "npm-run-all": "4.1.5",
    "protractor": "~7.0.0",
    "standard-version": "9.1.0",
    "ts-node": "~8.3.0",
    "tsickle": "^0.39.1",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "wait-on": "^5.2.1"
  }
}
